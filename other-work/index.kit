<!doctype html>
<html class="no-js" lang="">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>R.E.L</title>
		<meta name="description" content="">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">


		<link rel="apple-touch-icon" href="apple-touch-icon.png">
		<!-- Place favicon.ico in the root directory -->

		<link href='http://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic,700italic|Lobster|Open+Sans:400italic,400,700,600' rel='stylesheet' type='text/css'>
		<link rel="stylesheet" href="/css/normalize.css">
		<link rel="stylesheet" href="/css/main.css">

		<script src="js/vendor/modernizr-2.8.3.min.js"></script>
	</head>
	<body>
		<!-- @import '../partials/header.html' -->
		<div id="other-work" class="container-fluid">
			<section id="intro" class="row scroll-section">
				<div class="container home-container">
					<span class="other-title">
						i have worked across many mediums of design<br>
						<strong>fashion, web based graphics, logos</strong> and <strong>product packaging</strong>
						<span class="sub-title">
							&bull; see below some of the companies that i have worked with
						</span>
					</span>
				</div>
				<a class="scroll-button" onclick="$('#design').animatescroll();"></a>
			</section>
			<section id="design" class="row scroll-section">
				<div class="container">
					<span class="h7">Web based graphics, logos and product packaging</span>
					<div class="row">
						<div class="col-xs-12">
							<img src="/img/other-work-design.png" alt="">
						</div>
					</div>
				</div>
				<a class="scroll-button" onclick="$('#fashion').animatescroll();"></a>
			</section>
			<section id="fashion" class="row last-section">
				<div class="container">
					<span class="h7">Fashion design</span>
					<div class="row">
						<div class="col-xs-12">
							<div id="fashion-carousel" class="carousel slide" data-ride="carousel">
								  <!-- Indicators -->
								  <ol class="carousel-indicators">
									<li data-target="#fashion-carousel" data-slide-to="0" class="active"></li>
									<li data-target="#fashion-carousel" data-slide-to="1"></li>
									<li data-target="#fashion-carousel" data-slide-to="2"></li>
									<li data-target="#fashion-carousel" data-slide-to="3"></li>
									<li data-target="#fashion-carousel" data-slide-to="4"></li>
									<li data-target="#fashion-carousel" data-slide-to="5"></li>
									<li data-target="#fashion-carousel" data-slide-to="6"></li>
									<li data-target="#fashion-carousel" data-slide-to="7"></li>
									<li data-target="#fashion-carousel" data-slide-to="8"></li>
								  </ol>

								  <!-- Wrapper for slides -->
									<div class="carousel-inner" role="listbox">
										<div class="item active">
											<img src="/img/fashion/1.png">
										</div>

										<div class="item">
											<img src="/img/fashion/2.png">
										</div>

										<div class="item">
											<img src="/img/fashion/3.png">
										</div>

										<div class="item">
											<img src="/img/fashion/5.png">
										</div>

										<div class="item">
											<img src="/img/fashion/6.png">
										</div>

										<div class="item">
											<img src="/img/fashion/7.png">
										</div>

										<div class="item">
											<img src="/img/fashion/8.png">
										</div>

										<div class="item">
											<img src="/img/fashion/9.png">
										</div>
									</div>

									<!-- Left and right controls -->
									<a class="left carousel-control" href="#fashion-carousel" role="button" data-slide="prev">
										<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
										<span class="sr-only">Previous</span>
									</a>
									<a class="right carousel-control" href="#fashion-carousel" role="button" data-slide="next">
										<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
										<span class="sr-only">Next</span>
									</a>
								</div>
						</div>
					</div>
				</div>
				<a class="scroll-button" onclick="$('#contact').animatescroll();">TOP</a>
			</section>
			<!-- @import '../partials/footer.html' -->